#include <stdio.h>
int main()
{
    int n, max,i;
    printf("enter the no of  elements:");
    scanf("%d",&n);
    int arr[n];
    printf("enter the elements:");
    for(i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    }
    max=arr[0];
       for(i=1;i<n;i++)
   {
    if(arr[i]>max)max=arr[i];
}
    
    printf("the max value :%d\n",max);
    return 0;
}
