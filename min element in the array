#include <stdio.h>
int main()
{
    int n, min,i;
    printf("enter the no of  elements:");
    scanf("%d",&n);
    int arr[n];
    printf("enter the elements:");
    for(i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    }
    min=arr[0];
       for(i=1;i<n;i++)
   {
    if(arr[i]<min)min=arr[i];
}
    
    printf("the min value :%d\n",min);
    return 0;
}
